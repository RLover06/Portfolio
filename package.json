    "@sveltejs/adapter-static": "^2.0.1",
    "@sveltejs/kit": "^1.5.0",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "gh-pages": "^5.0.0",
    "postcss": "^8.4.31",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "svelte": "^4.2.9",
    "svelte-check": "^3.0.1",
    "tailwindcss": "^3.3.5",
    "tslib": "^2.4.1",
